@model IEnumerable<Domain.Entidades.UsuarioTipo>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NomeTipo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Acesso)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Ativo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DataAlteracao)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NomeTipo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Acesso)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Ativo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DataAlteracao)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdTipoUsuario">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdTipoUsuario">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdTipoUsuario">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
